#################################
#          B A N N E R          #
#################################
01 = '1;31m'                    #
02 = '1;34m'                    #
03 = '0;36m'                    #
04 = '\e[95m'                   #
05 = '1;35m'                    #
06 = '0;37m'                    #
07 = '1;36m'                    #
08 = '1;33m'                    #
09 = '\e[5m'                    #
10 = '0;33m'                    #
11 = '\e[25m'                   #
12 = '1;37;41m'                 #
#################################

clear
        printf "${purple}


'########:'##::: ##:'########:'##::::'##:'########:'########::'##::: ##:'########:'########:>
 ##.....:: ###:: ##:... ##..:: ##:::: ##: ##.....:: ##.... ##: ###:: ##: ##.....::....||..:::

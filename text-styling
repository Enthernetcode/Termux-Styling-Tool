        printf " \e[0m\e[1;91m[\e[0m\e[1;97m01\e[0m\e[1;91m]\e[0m\e[1;32m red        \e[0m\n\n"
        printf " \e[0m\e[1;91m[\e[0m\e[1;97m02\e[0m\e[1;91m]\e[0m\e[1;32m blue       \e[0m\n\n"
        printf " \e[0m\e[1;91m[\e[0m\e[1;97m03\e[0m\e[1;91m]\e[0m\e[1;32m cyan       \e[0m\n\n"
        printf " \e[0m\e[1;91m[\e[0m\e[1;97m04\e[0m\e[1;91m]\e[0m\e[1;32m pink       \e[0m\n\n"
        printf " \e[0m\e[1;91m[\e[0m\e[1;97m05\e[0m\e[1;91m]\e[0m\e[1;32m purple     \e[0m\n\n"
        printf " \e[0m\e[1;91m[\e[0m\e[1;97m06\e[0m\e[1;91m]\e[0m\e[1;32m green      \e[0m\n\n"
        printf " \e[0m\e[1;91m[\e[0m\e[1;97m07\e[0m\e[1;91m]\e[0m\e[1;32m cyan1      \e[0m\n\n"
        printf " \e[0m\e[1;91m[\e[0m\e[1;97m08\e[0m\e[1;91m]\e[0m\e[1;32m yellow     \e[0m\n\n"
        printf " \e[0m\e[1;91m[\e[0m\e[1;97m09\e[0m\e[1;91m]\e[0m\e[1;32m sta        \e[0m\n\n"
